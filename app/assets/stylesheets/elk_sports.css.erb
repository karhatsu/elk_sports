body {
    background-color: olive;
    font-family: monospace;
    font-size: 12px;
}

a {
    color: olive;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}

h2 {
    margin-top: 10px;
    font-size: 16px;
}

h3 {
    font-size: 14px;
}

form {
    margin: 0px;
}

input[type='number'] {
    width: 40px;
}

input[type='number'].max2 {
  width: 30px;
}

div.sub_element {
    background-color: #DDEEB5;
    padding: 5px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}

div.sub_form {
    margin-bottom: 5px;
}

div.sub_sub_element {
    margin-top: 2px;
    background-color: white;
    padding: 3px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}

div.sub_sub_element h3 {
    margin-top: 2px;
    margin-bottom: 2px;
    font-weight: normal;
    font-size: 12px;
}

div.age_groups_element {
    width: 80%;
}

div.relay_competitors_element {
    font-size: 80%;
}

div.relay_competitors_element input {
    height: 13px;
}

div.action_box {
    background-image: url(<%= image_path 'action_arrow.png' %>);
    background-repeat: no-repeat;
    background-position: 5px;
    border: dashed 1px gray;
    padding: 5px 5px 5px 20px;
    margin: 5px 0;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}

div.question {
    font-weight: bold;
    color: olive;
    cursor: pointer;
    margin-top: 5px;
}

div.question .highligth {
    color: orange;
}

div.answer {
    border: 1px solid black;
    border-radius: 3px;
    box-shadow: 5px 4px 5px #666666;
    padding: 4px;
    display: table;
    margin: 3px 0 15px 0;
}

div.left_column {
    float: left;
}

div.right_column {
    float: right;
}

.help {
    background-color: olive;
    color: white;
    border: olive 1px solid;
    padding: 2px;
    cursor: pointer;
    font-weight: bold;
    font-family: arial;
    font-size: 12px;
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
}

a.help:hover {
    text-decoration: none;
    background-color: white;
    color: olive;
}

.center {
    text-align: center;
}

.series_best_time {
    font-weight: bold;
}

.total_points {
    font-weight: bold;
}

#company_contact_corner {
    position: absolute;
    right: 10px;
    top: 10px;
}

.company_contact {
    text-align: center;
    border: 1px solid olive;
    padding: 5px;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    box-shadow: 5px 4px 5px #666666;
    -moz-box-shadow: 5px 4px 5px #666666;
    -webkit-box-shadow: 5px 4px 5px #666666;
    width: 200px;
}

.company_contact .title {
    font-weight: bold;
}

div.main_page_columns {
    margin-top: 10px;
    width: 1040px;
}

div.title_box {
    border: 1px solid olive;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    box-shadow: 5px 4px 5px #666666;
    -moz-box-shadow: 5px 4px 5px #666666;
    -webkit-box-shadow: 5px 4px 5px #666666;
    padding: 0;
}

div.title_box .box_title {
    background-color: olive;
    color: white;
    font-family: Arial;
    font-size: 16px;
    font-weight: bold;
    padding: 5px;
    text-align: center;
    text-transform: uppercase;
}

div.special_box .box_title {
    background-color: orange;
}

.title_box_content {
    padding: 5px;
}

#races_box {
    width: 655px;
    margin-bottom: 10px;
}

#races_box .box_sub_title, .prices_section .box_sub_title {
    background-color: #DDEEB5;
    font-size: 13px;
    margin: 3px 0;
    padding: 3px;
    border-radius: 2px;
    -moz-border-radius: 2px;
    -webkit-border-radius: 2px;
}

div.all_races_link {
    margin: 5px 0 0;
}

.home_page_fb_like {
    margin-bottom: 15px;
    width: 360px;
}

.commercial_box {
    margin-bottom: 15px;
    text-align: center;
    width: 360px;
}

.commercial_box .announcements {
    text-align: left;
}

.title_box .links {
    margin: 15px 3px;
}

.commercial_box_link {
    background-color: olive;
    color: white;
    padding: 5px 10px;
    border: 1px solid olive;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
}

.commercial_box_link:hover {
    background-color: white;
    color: olive;
    text-decoration: none;
}

div.special_box .commercial_box_link {
    background-color: orange;
    border: 1px solid orange;
}

div.special_box .commercial_box_link:hover {
    background-color: white;
    color: olive;
}

.changing_text_wrapper {
    background-color: #DDEEB5;
    padding: 5px;
    height: 30px;
    margin: 5px;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
}

.changing_text_wrapper .keyword {
    font-weight: bold;
}

.info_page_box {
    float: left;
    margin: 5px 20px 15px;
    text-align: center;
    width: 300px;
}

.info_page_big_box {
    float: left;
    margin: 5px 55px 15px;
    text-align: center;
    width: 400px;
}

#registration_form {
    float: left;
}

#login_link_box {
    float: left;
    margin: 10px 0 0 100px;
    text-align: center;
    width: 300px;
}

th {
    text-align: left;
    padding: 2px 5px;
}

td {
    padding: 2px 5px;
}

div.section {
    border: 1px solid black;
    padding: 4px;
    display: table;
    margin-bottom: 8px;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    box-shadow: 5px 4px 5px #666666;
    -moz-box-shadow: 5px 4px 5px #666666;
    -webkit-box-shadow: 5px 4px 5px #666666;
}

.team_name, .team_points {
    font-weight: bold;
}

ul.positive li {
    list-style-image: url(<%= image_path 'icon_yes.gif' %>);
}

.explanation {
    cursor: help;
}

pre.file_example {
    border: black solid 1px;
    padding: 3px;
    background-color: #D9D9D9;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
}

span.unofficial {
    font-style: italic;
    font-size: 80%;
    cursor: help;
}

span.adjustment {
    cursor: help;
}

tr.odd {
    background-color: #DDEEB5;
}

#reload_counter {
  background-color: #DDEEB5;
  margin-top: 5px;
  padding: 3px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
}

.full_width {
  width: 880px;
}

.cup_race {
  padding-left: 30px;
}

.read_only_form_row {
  margin: 3px 0;
}

.form_label {
  background-color: #DDEEB5;
  border-radius: 2px;
  display: inline-block;
  text-align: right;
  padding: 3px;
  vertical-align: top;
}

#activation_key_form .form_label { width: 450px; }
#activation_key_form_short .form_label { width: 100px; }
.club_form .form_label { width: 100px; }
.cup_form .form_label { width: 350px; }
.cup_form_races .form_label { text-align: left; }
.cup_series_sub_form .form_label { display: inline; }
.feedback_form .form_label { width: 100px; }
.invite_officials_form .form_label { width: 450px; }
.license_form .form_label { width: 300px; }
.login_form .form_label { width: 150px; }
.race_form .form_label { width: 280px; }
.relay_form .form_label { width: 150px; }
.team_competition_form .form_label { width: 450px; }
.user_readonly_form .form_label { width: 100px; }
.user_form .form_label { width: 150px; }
.video_form .form_label { width: 100px; }

.competitors_table .header_row .title {
  display: inline-block;
  font-weight: bold;
  *display: inline;
}

.competitors_table .competitor_row .value {
  display: inline-block;
  *display: inline;
}

#competitors_table .number { width: 40px; }
#competitors_table .start_time { width: 150px; }
#competitors_table .first_name { width: 100px; }
#competitors_table .last_name { width: 140px; }
#competitors_table .club { width: 140px; }
#competitors_table .series { width: 140px; }
#competitors_table .series select { width: 135px; }
#competitors_table .age_group { width: 80px; }
#competitors_table .age_group select { width: 75px; }
#competitors_table .button { width: 95px; }

#competitors_table .after_action {
  background-color: #DDEEB5;
}

#estimates_table .number { width: 40px; }
#estimates_table .name { width: 200px; }
#estimates_table .estimate { width: 70px; }

#shots_table .number { width: 40px; }
#shots_table .name { width: 200px; }
#shots_table .total_shots { margin-left: 10px; width: 50px; }
#shots_table .individual_shots { width: 425px; }
#shots_table .result { width: 40px; }

#times_table .number { width: 40px; }
#times_table .name { width: 200px; }
#times_table .time { width: 200px; }

#video_page .video_description {
  margin-bottom: 10px;
}
#video_page .column {
  float: left;
}
#video_page .results_column {
  margin-left: 10px;
  overflow: hidden;
  width: 450px;
}
#video_page h3 {
  margin-bottom: 5px;
}
#video_page .series_menu {
  margin-bottom: 5px;
  text-align: right;
}
#video_page #refresh_link_wrapper {
  margin-bottom: 5px;
  text-align: right;
}

.title-and-fb-share {
  position: relative;
}

.title-and-fb-share .fb-share-button {
  position: absolute;
  top: 0px;
  left: 950px;
}

.title-and-fb-share.race-page .fb-share-button {
  top: 10px;
}

.race-page-column {
  float: left;
}

.race-page-column .section {
  margin-bottom: 30px;
}

.cup_results {
  overflow: scroll;
  width: 1025px;
}

.official-page-column-left {
  float: left;
  width: 60%;
}

.official-page-column-right {
  float: left;
  width: 40%;
}
