a {
    color: olive;
    text-decoration: none;
}

a:hover {
    text-decoration: underline;
}

h2:first-of-type {
  margin-top: 8px;
}

#district_menu {
  margin-bottom: 10px;
}

.center {
    text-align: center;
}

.series_best_time {
    font-weight: bold;
}

.total_points {
    font-weight: bold;
}

.home_page_fb_like {
    margin-bottom: 15px;
    width: 360px;
}

.team_name, .team_points {
    font-weight: bold;
}

.explanation {
    cursor: help;
}

pre.file_example {
    border: black solid 1px;
    padding: 3px;
    background-color: #D9D9D9;
    border-radius: 3px;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
}

span.unofficial {
    font-style: italic;
    font-size: 80%;
    cursor: help;
}

span.adjustment {
    cursor: help;
}

#reload_counter {
  background-color: #DDEEB5;
  margin-top: 5px;
  padding: 3px;
  border-radius: 3px;
  -moz-border-radius: 3px;
  -webkit-border-radius: 3px;
  width: 100%;
  text-align: center;
}

.cup_results {
  overflow: scroll;
  max-width: 1025px;
}

.result_rotation_sub_selection {
  margin-left: 20px;
}

.announcement {
  margin-bottom: 24px;
}

.before_facebook_like_box {
  height: 16px;
}

.pagination {
  margin: 8px 0;
}

.result-place-table {
  margin-bottom: 16px;
}
