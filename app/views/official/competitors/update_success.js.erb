$("#update_error").hide();
$("#update_error").html("");
updateInlineMessage("#status_<%= @competitor.id %>", 'success', '<%= t :saved %>')
$("#competitor_old_values_estimate1").val(<%= @competitor.estimate1 %>);
$("#competitor_old_values_estimate2").val(<%= @competitor.estimate2 %>);
$("#competitor_old_values_estimate3").val(<%= @competitor.estimate3 %>);
$("#competitor_old_values_estimate4").val(<%= @competitor.estimate4 %>);
$("#time_<%= @competitor.id %>").html("<%= @competitor.time_in_seconds ? time_from_seconds(@competitor.time_in_seconds) : '' %>")
$("#estimate_points_<%= @competitor.id %>").html("<%= @competitor.estimate_points %>")
$("#shots_result_<%= @competitor.id %>").html("<%= @competitor.shooting_score %>")
setTimeout(function() {
  resetInlineMessage("#status_<%=@competitor.id%>")
}, 5000);
