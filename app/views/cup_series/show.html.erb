<% content_for(:title, "#{@cup_series.cup.name} - #{long_cup_series_name(@cup_series)} - #{t :results}") %>
<div class="title-and-fb-share">
  <h2><%= "#{long_cup_series_name(@cup_series)} - #{t :results}" %></h2>
  <%= render partial: 'shared/facebook_share' %>
</div>
<% content_for :sub_sub_menu do %>
  <%= render :partial => 'menus/cup_series_sub_menu' %>
<% end %>
<div class="info"><%= render partial: 'cups/points_rule', locals: {cup: @cup} %></div>
<div class="cup_results">
  <div class="section">
    <%= render :partial => 'cup_series_results' %>
  </div>
</div>
<div class="action_box">
  <%= link_to t(:download_results_pdf), cup_cup_series_path(@cup, @cup_series, :format => :pdf) %>
</div>
<hr/>
<%= link_to t(:back_to_cup_page), cup_path(@cup) %>
