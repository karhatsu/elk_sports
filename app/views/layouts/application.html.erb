<!DOCTYPE html>
<html lang="fi">
  <head>
    <meta charset="UTF-8"/>
    <%= render :partial => 'shared/title' %>
    <%= stylesheet_link_tag 'application' %>
    <% if Mode.offline? %>
      <%= javascript_include_tag 'application-offline' %>
    <% else %>
      <%= javascript_include_tag 'application' %>
    <% end %>
    <% if facebook_env? %>
      <%= javascript_include_tag 'social' %>
    <% end %>
    <%= csrf_meta_tag %>
    <%= render :partial => 'shared/google_analytics' %>
    <%= render 'shared/facebook_meta' %>
  </head>
  <%= render :partial => 'shared/body' %>
</html>
